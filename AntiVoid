-- This is the script for no void
local blockPosition = Vector3.new(0, 235, 0)  -- Change this to your desired coordinates

-- Define the teleport destination
local teleportPosition = Vector3.new(-0.76, 293.09, -310.20)  -- Updated teleport location

-- Function to create and place the block
local function placeBlock(position)
    -- Create a new Part
    local block = Instance.new("Part")
    block.Size = Vector3.new(9999999999999999999, 4, 99999999999999999999)  -- Set the size of the block (reasonable size)
    block.Position = position  -- Set the position of the block
    block.Anchored = true  -- Make sure the block doesn't fall
    block.Transparency = 1  -- Make the block invisible
    block.CanCollide = false  -- Optional: Make sure the block doesn't collide with other objects
    block.Touched:Connect(function(hit)
        local character = hit.Parent
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            local player = game.Players:GetPlayerFromCharacter(character)
            if player and player == game.Players.LocalPlayer then
                -- Teleport only the local player
                player.Character:SetPrimaryPartCFrame(CFrame.new(teleportPosition))
            end
        end
    end)
    block.Parent = workspace  -- Parent the block to the workspace so it appears in the game
end

-- Call the function to place the block at the specified position
placeBlock(blockPosition)
